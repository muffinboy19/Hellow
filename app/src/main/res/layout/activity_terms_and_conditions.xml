<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="termsAndConditionsVM" type="com.gauravsapplication.app.modules.termsandconditions.data.viewmodel.TermsAndConditionsVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearTermsAndCondi"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_51">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearIPhoneXorne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLeftSide"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_width="@dimen/_54pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_leftside"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_leftside"/>
                <ImageView android:id="@+id/imageRightSide"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_271pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_66pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rightside"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rightside"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowarrowleft"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="@dimen/_12pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTermAndCondition"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_60pxh"
                android:layout_weight="0.95" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{termsAndConditionsVM.termsAndConditionsModel.txtTermAndCondition}"
                style="@style/txtGilroysemibold24" tools:text="@string/msg_terms_and_condi"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLoremipsum"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{termsAndConditionsVM.termsAndConditionsModel.txtLoremipsum}"
            style="@style/txtGilroysemibold18" textUnderline="true" tools:text="@string/lbl_lorem_ipsum"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtDescription"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_10pxh"
            android:text="@{termsAndConditionsVM.termsAndConditionsModel.txtDescription}"
            style="@style/txtGilroymedium16_3" tools:text="@string/msg_lorem_ipsum_dol"/>
        <View android:background="@color/bluegray_100"
            android:id="@+id/lineLineSeven"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_396pxh"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLoremipsumOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{termsAndConditionsVM.termsAndConditionsModel.txtLoremipsumOne}"
            style="@style/txtGilroysemibold18" textUnderline="true" tools:text="@string/lbl_lorem_ipsum"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtDescriptionOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_10pxh"
            android:text="@{termsAndConditionsVM.termsAndConditionsModel.txtDescriptionOne}"
            style="@style/txtGilroymedium16_3" tools:text="@string/msg_lorem_ipsum_dol"/>
        <View android:background="@color/bluegray_100"
            android:id="@+id/lineLineEight"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_396pxh"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLoremipsumTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{termsAndConditionsVM.termsAndConditionsModel.txtLoremipsumTwo}"
            style="@style/txtGilroysemibold18" textUnderline="true" tools:text="@string/lbl_lorem_ipsum"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtDescriptionTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_10pxh"
            android:text="@{termsAndConditionsVM.termsAndConditionsModel.txtDescriptionTwo}"
            style="@style/txtGilroymedium16_3" tools:text="@string/msg_lorem_ipsum_dol"/>
        <View android:background="@color/bluegray_100"
            android:id="@+id/lineLineSix"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_396pxh"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLoremipsumThree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="@{termsAndConditionsVM.termsAndConditionsModel.txtLoremipsumThree}"
            style="@style/txtGilroysemibold18" textUnderline="true" tools:text="@string/lbl_lorem_ipsum"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtDescriptionThree"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_10pxh"
            android:text="@{termsAndConditionsVM.termsAndConditionsModel.txtDescriptionThree}"
            style="@style/txtGilroymedium16_3" tools:text="@string/msg_lorem_ipsum_dol"/>
    </LinearLayout>
</layout>
