                android:id="@+id/txtEditProfile"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_122pxh"
                android:layout_weight="0.91" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtEditProfile}"
                style="@style/txtGilroysemibold24" tools:text="@string/lbl_edit_profile"/>
